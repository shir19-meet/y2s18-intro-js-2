                                        <amp-img width="4" height="2"
                                            layout="responsive"
                                            src="https://maps.googleapis.com/maps/api/staticmap?center=51.165691,10.451526&zoom=16&size=600x300&maptype=roadmap&markers=color:red|label:S|51.165691,10.451526">
